{
  "name": "@tripled/react-ui",
  "version": "0.0.1-alpha.1",
  "description": "A UI component library for react project that follow Diamond Design's guidelines",
  "author": "@tripled-labs",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "packageManager": "yarn@3.5.1",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint src tests stories --ext .ts,.tsx --fix",
    "dev:sb": "storybook dev -p 6006",
    "build:sb": "storybook build",
    "build": "rollup -c",
    "clean": "node scripts/clean-dist.js"
  },
  "imports": {
    "#components/*": [
      "./src/components/*"
    ],
    "#styles/*": [
      "./src/styles/*"
    ]
  },
  "devDependencies": {
    "@nexbot/eslint-config": "^1.0.1",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@storybook/addon-essentials": "7.0.17",
    "@storybook/addon-interactions": "7.0.17",
    "@storybook/addon-links": "7.0.17",
    "@storybook/addon-styling": "^1.0.8",
    "@storybook/blocks": "7.0.17",
    "@storybook/builder-vite": "^7.0.17",
    "@storybook/react": "7.0.17",
    "@storybook/react-vite": "^7.0.17",
    "@storybook/testing-library": "0.0.14-next.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.7",
    "autoprefixer": "^10.4.14",
    "clsx": "^1.2.1",
    "eslint": "^8.41.0",
    "eslint-plugin-storybook": "^0.6.12",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.24",
    "prop-types": "15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.23.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "7.0.17",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4",
    "vite": "^4.4.4",
    "vitest": "^0.33.0"
  },
  "peerDependencies": {
    "clsx": "^1.2.1",
    "react": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "diamond-design",
    "diamond-ui",
    "diamond",
    "tripled-ui",
    "tripled",
    "triple-d",
    "ddd",
    "react",
    "react-ui",
    "tailwind",
    "css",
    "ui",
    "ux",
    "ui-ux",
    "ts",
    "typescript"
  ],
  "homepage": "https://github.com/tripled-labs/react-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tripled-labs/react-ui.git"
  },
  "bugs": {
    "url": "https://github.com/tripled-labs/react-ui/issues"
  },
  "files": [
    "dist/"
  ]
}
